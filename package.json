    "echarts": "latest",
    "echarts-gl": "latest",
    "next": "latest",
    "react": "latest"
  },
  "devDependencies": {
    "@cypress/code-coverage": "latest",
    "@this-dot/cypress-indexeddb": "latest",
    "@types/node": "latest",
    "@types/react": "latest",
    "babel-plugin-istanbul": "latest",
    "cypress": "latest",
    "eslint": "latest",
    "eslint-config-next": "latest",
    "fs": "latest",
    "typescript": "latest"
  }
